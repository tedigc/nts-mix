{
  "name": "nts-app",
  "version": "1.0.0",
  "description": "A web application for compiling playlists for NTS radio shows",
  "main": "server/src/index.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "react-scripts build",
    "server": "nodemon server/src/index.js --variables --exec babel-node",
    "client": "cd client && npm start",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha client/test --timeout 20000  --compilers js:babel-core/register"
  },
  "author": "tedigc",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.2",
    "cheerio": "^1.0.0-rc.2",
    "create-react-app": "^1.4.3",
    "express": "^4.16.2",
    "google-auth-library": "^0.12.0",
    "googleapis": "^23.0.0",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "styled-components": "^2.2.3"
  }
}
